<?xml version="1.0" encoding="utf-8"?>
<selector
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:state_selected="true">
        <shape>
            <gradient
                android:startColor="@color/SF_Color13_Start"
                android:endColor="@color/SF_Color13_End"
                android:angle="-90.0" />
            <corners
                android:radius="6.0dip" />
        </shape>
    </item>
    <item>
        <shape>
            <stroke
                android:width="1.0dip"
                android:color="@color/SF_Color12" />
            <corners
                android:radius="6.0dip" />
        </shape>
    </item>
</selector>