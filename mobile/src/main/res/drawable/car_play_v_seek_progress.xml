    <item
        android:id="@android:id/background">
        <shape>
            <solid
                android:color="@color/UTColor01_10" />
            <corners
                android:radius="10000.0dip" />
        </shape>
    </item>
    <item
        android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <solid
                    android:color="#00ffffff" />
                <corners
                    android:radius="10000.0dip" />
            </shape>
        </clip>
    </item>
    <item
        android:id="@android:id/progress">
        <clip>
            <shape>
                <solid
                    android:color="#ff4791ff" />
                <corners
                    android:radius="10000.0dip" />
            </shape>
        </clip>
    </item>
</layer-list>